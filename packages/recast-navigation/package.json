{
  "name": "recast-navigation",
  "description": "Recast Navigation for JavaScript!",
  "keywords": [
    "recastnavigation",
    "recast-navigation",
    "recast",
    "detour",
    "navmesh",
    "pathfinding",
    "navigation",
    "crowd-simulation",
    "wasm",
    "webassembly"
  ],
  "author": {
    "name": "Isaac Mason",
    "email": "isaac@isaacmason.com",
    "url": "https://isaacmason.com/"
  },
  "version": "0.40.0",
  "license": "MIT",
  "homepage": "https://github.com/isaac-mason/recast-navigation-js",
  "bugs": {
    "url": "https://github.com/isaac-mason/recast-navigation-js/issues"
  },
  "type": "module",
  "main": "./index.mjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "default": "./index.mjs"
    },
    "./generators": {
      "types": "./generators.d.ts",
      "import": "./generators.mjs",
      "default": "./generators.mjs"
    }
  },
  "files": [
    "index.mjs",
    "index.mjs.map",
    "index.d.ts",
    "generators.mjs",
    "generators.mjs.map",
    "generators.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "(rm index.mjs index.d.ts generators.mjs generators.d.ts || true)",
    "build": "yarn clean && rollup --config rollup.config.js --bundleConfigAsCjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "tsc && vitest run",
    "lint": "eslint src"
  },
  "dependencies": {
    "@recast-navigation/core": "0.40.0",
    "@recast-navigation/generators": "0.40.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@isaac-mason/eslint-config-typescript": "^0.0.9",
    "@mdx-js/react": "^2.3.0",
    "@pmndrs/assets": "^1.6.0",
    "@react-three/drei": "^9.105.4",
    "@react-three/fiber": "^8.16.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-actions": "^8.0.9",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-mdx-gfm": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.2.8",
    "@types/three": "^0.172.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.6.0",
    "leva": "^0.9.35",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "rollup": "^4.14.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^10.0.0",
    "storybook": "^8.0.9",
    "suspend-react": "^0.1.3",
    "three": "^0.172.0",
    "three-pathfinding": "^1.2.0",
    "typescript": "^5.4.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.2",
    "zustand": "^4.5.2"
  },
  "packageManager": "yarn@3.2.2"
}
