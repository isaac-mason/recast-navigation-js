{
  "name": "recast-navigation",
  "description": "Recast Navigation for JavaScript!",
  "keywords": [
    "recastnavigation",
    "recast-navigation",
    "recast",
    "detour",
    "navmesh",
    "pathfinding",
    "navigation",
    "crowd-simulation",
    "wasm",
    "webassembly"
  ],
  "author": {
    "name": "Isaac Mason",
    "email": "isaac@isaacmason.com",
    "url": "https://isaacmason.com/"
  },
  "version": "0.42.1",
  "license": "MIT",
  "homepage": "https://github.com/isaac-mason/recast-navigation-js",
  "bugs": {
    "url": "https://github.com/isaac-mason/recast-navigation-js/issues"
  },
  "type": "module",
  "main": "./index.mjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "default": "./index.mjs"
    },
    "./generators": {
      "types": "./generators.d.ts",
      "import": "./generators.mjs",
      "default": "./generators.mjs"
    }
  },
  "files": [
    "index.mjs",
    "index.mjs.map",
    "index.d.ts",
    "generators.mjs",
    "generators.mjs.map",
    "generators.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "(rm index.mjs index.d.ts generators.mjs generators.d.ts || true)",
    "build": "yarn clean && rollup --config rollup.config.js --bundleConfigAsCjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "tsc && vitest run"
  },
  "dependencies": {
    "@recast-navigation/core": "0.42.1",
    "@recast-navigation/generators": "0.42.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@mdx-js/react": "^3.1.0",
    "@pmndrs/assets": "^1.6.0",
    "@react-three/drei": "^10.4.2",
    "@react-three/fiber": "^9.2.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-links": "^9.0.15",
    "@storybook/react-vite": "^9.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "^0.178.0",
    "@vitejs/plugin-react": "^4.6.0",
    "leva": "^0.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.44.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-filesize": "^10.0.0",
    "storybook": "^9.0.15",
    "suspend-react": "^0.1.3",
    "three": "^0.178.0",
    "three-pathfinding": "^1.3.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vitest": "^3.2.4",
    "zustand": "^5.0.6"
  },
  "packageManager": "yarn@3.2.2"
}
